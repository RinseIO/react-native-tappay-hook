{
  "name": "react-native-tappay-hook",
  "version": "0.1.12",
  "description": "tappay for react hook",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/commonjs/index",
      "import": "./lib/module/index",
      "types": "./lib/typescript/index.d.ts",
      "node": "./lib/commonjs/index",
      "default": "./lib/commonjs/index"
    },
    "./oop": {
      "require": "./lib/commonjs/oop/index",
      "import": "./lib/module/oop/index",
      "types": "./lib/typescript/oop/index.d.ts",
      "node": "./lib/commonjs/oop/index",
      "default": "./lib/commonjs/oop/index"
    },
    "./oop/*": {
      "require": "./lib/commonjs/oop/*",
      "import": "./lib/module/oop/*",
      "types": "./lib/typescript/oop/*.d.ts",
      "node": "./lib/commonjs/oop/*",
      "default": "./lib/commonjs/oop/*"
    },
    "./fp": {
      "require": "./lib/commonjs/fp/index",
      "import": "./lib/module/fp/index",
      "types": "./lib/typescript/fp/index.d.ts",
      "node": "./lib/commonjs/fp/index",
      "default": "./lib/commonjs/fp/index"
    },
    "./fp/*": {
      "require": "./lib/commonjs/fp/*",
      "import": "./lib/module/fp/*",
      "types": "./lib/typescript/fp/*.d.ts",
      "node": "./lib/commonjs/fp/*",
      "default": "./lib/commonjs/fp/*"
    },
    "./hooks": {
      "require": "./lib/commonjs/hooks/index",
      "import": "./lib/module/hooks/index",
      "types": "./lib/typescript/hooks/index.d.ts",
      "node": "./lib/commonjs/hooks/index",
      "default": "./lib/commonjs/hooks/index"
    },
    "./hooks/*": {
      "require": "./lib/commonjs/hooks/*",
      "import": "./lib/module/hooks/*",
      "types": "./lib/typescript/hooks/*.d.ts",
      "node": "./lib/commonjs/hooks/*",
      "default": "./lib/commonjs/hooks/*"
    },
    "./components": {
      "require": "./lib/commonjs/components/index",
      "import": "./lib/module/components/index",
      "types": "./lib/typescript/components/index.d.ts",
      "node": "./lib/commonjs/components/index",
      "default": "./lib/commonjs/components/index"
    },
    "./components/*": {
      "require": "./lib/commonjs/components/*",
      "import": "./lib/module/components/*",
      "types": "./lib/typescript/components/*.d.ts",
      "node": "./lib/commonjs/components/*",
      "default": "./lib/commonjs/components/*"
    },
    "./images": {
      "require": "./lib/commonjs/images/index",
      "import": "./lib/module/images/index",
      "types": "./lib/typescript/images/index.d.ts",
      "node": "./lib/commonjs/images/index",
      "default": "./lib/commonjs/images/index"
    },
    "./images/*.png": {
      "require": "./lib/commonjs/images/*.png",
      "import": "./lib/module/images/*.png",
      "types": "./lib/module/images/*.png",
      "node": "./lib/commonjs/images/*.png",
      "default": "./lib/commonjs/images/*.png"
    }
  },
  "react-native": "src/index",
  "source": "src",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install && yarn example pods",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build",
    "postinstall": "unzip -o ios/TPDirect.xcframework.zip -d ios"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "tappay",
    "hooks",
    "react-hooks"
  ],
  "repository": "https://github.com/archer102125220/react-native-tappay-hook",
  "author": "Parker Chan <archer102125220.2015@gmail.com> (https://github.com/archer102125220)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archer102125220/react-native-tappay-hook/issues"
  },
  "homepage": "https://github.com/archer102125220/react-native-tappay-hook#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^28.1.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^29.5.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.6",
    "react-native-builder-bob": "^0.20.0",
    "release-it": "^15.0.0",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-native": "^0.70.5"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "^yarn@1.22.15",
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
